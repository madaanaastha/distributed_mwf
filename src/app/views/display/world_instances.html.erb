<div class="col-lg-12">
  <div class="col-lg-6 col-lg-offset-3">
    <div class="center-block"><h1><%= @world_instance.name %></h1></div>
  </div>
  <%= render 'display/world_instances_operations'%>
</div>
<% if @superclass.is_external %>
    <h4 align="center"> is of type <a href="<%= @s_world.home_location_uri %>"><%= @superclass.name %></a></h4>
<%else%>
    <h4 align="center"> is of type <a href="<%= @superclass.get_uri %>"><%= @superclass.name %></a></h4>
<%end%>
<% if @container.is_external %>
    <h4 align="center"> is contained inside <a href="<%= @c_world.home_location_uri %>"><%= @container.name %></a></h4><br>
<%else%>
    <h4 align="center"> is contained inside <a href="<%= @container.get_uri %>"><%= @container.name %></a></h4><br>
<%end%>
<div class="col-lg-6">
  <div class="panel panel-info">
    <div class="panel-heading">
      <h3 class="panel-title">Description</h3>
    </div>
    <div class="panel-body">
      <ul class="list-group">
        <li class="list-group-item"><%= @world_instance.description %></li>
        <li class="list-group-item"><%= @world_instance.name %> was created at <%= @world_instance.created_at %></li>
        <li class="list-group-item"><%= @world_instance.name %> was last updated at <%= @world_instance.updated_at %></li>
      </ul>
    </div>
  </div>
</div>
<div class="col-lg-6">
  <div class="panel panel-info">
    <div class="panel-heading">
      <h3 class="panel-title">Attributes</h3>
    </div>
    <div class="panel-body">
      <% attrs =eval(@world_instance.attrs) unless @world_instance.attrs.nil? %>
      <% unless attrs.nil? %>
          <% attrs.each do |key, val| %>
              <%= key %> => <%= val %>
              <br>
          <% end %>
      <% end %>
    </div>
  </div>
</div>

